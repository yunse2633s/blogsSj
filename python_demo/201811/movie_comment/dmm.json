ue,\"supportLike\":true,\"sureViewed\":1,\"tagList\":{\"fixed\":[{\"id\":1,\"name\":\"好评\"},{\"id\":4,\"name\":\"购票\"}]},\"time\":\"2018-11-29 09:59\",\"userId\":294985789,\"userLevel\":3,\"videoDuration\":0,\"vipType\":0},{\"approve\":1,\"approved\":false,\"assistAwardInfo\":{\"avatar\":\"\",\"celebrityId\":0,\"celebrityName\":\"\",\"rank\":0,\"title\":\"\"},\"authInfo\":\"\",\"avatarurl\":\"https://img.meituan.net/maoyanuser/350bff22b2ac2c7e5b11937b8dc8f9d212207.png\",\"cityName\":\"武汉\",\"content\":\"挺不错的一部电影，有意义\",\"filmView\":false,\"id\":1046107371,\"isMajor\":false,\"juryLevel\":0,\"majorType\":0,\"movieId\":1208282,\"nick\":\"AxY719506771\",\"nickName\":\"AxY719506771\",\"oppose\":0,\"pro\":false,\"reply\":0,\"score\":5,\"spoiler\":0,\"startTime\":\"2018-11-29 09:59:40\",\"supportComment\":true,\"supportLike\":true,\"sureViewed\":0,\"tagList\":{\"fixed\":[{\"id\":1,\"name\":\"好评\"},{\"id\":4,\"name\":\"购票\"}]},\"time\":\"2018-11-29 09:59\",\"userId\":111187254,\"userLevel\":2,\"videoDuration\":0,\"vipInfo\":\"\",\"vipType\":0},{\"approve\":0,\"approved\":false,\"assistAwardInfo\":{\"avatar\":\"\",\"celebrityId\":0,\"celebrityName\":\"\",\"rank\":0,\"title\":\"\"},\"authInfo\":\"\",\"avatarurl\":\"https://img.meituan.net/avatar/6e478e0d7677468a32118973ffa9d125219685.jpg\",\"cityName\":\"佛山\",\"content\":\"以荒诞的喜剧反映了社会底层人民的生活方式与底层人民的生存环境，同时引起我们的共鸣，我们在某一程度上也是与他们一样的无名之辈。\",\"filmView\":false,\"gender\":1,\"id\":1046102823,\"isMajor\":false,\"juryLevel\":0,\"majorType\":0,\"movieId\":1208282,\"nick\":\"Ares2460\",\"nickName\":\"Ares\",\"oppose\":0,\"pro\":false,\"reply\":0,\"score\":5,\"spoiler\":0,\"startTime\":\"2018-11-29 09:59:08\",\"supportComment\":true,\"supportLike\":true,\"sureViewed\":1,\"tagList\":{\"fixed\":[{\"id\":1,\"name\":\"好评\"},{\"id\":4,\"name\":\"购票\"}]},\"time\":\"2018-11-29 09:59\",\"userId\":696583526,\"userLevel\":3,\"videoDuration\":0,\"vipInfo\":\"\",\"vipType\":0},{\"approve\":0,\"approved\":false,\"assistAwardInfo\":{\"avatar\":\"\",\"